<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.albert.weightapp"
          android:versionCode="1"
          android:versionName="1.0">

    <application
            android:allowBackup="true"
            android:icon="@drawable/barbellic"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.albert.weightapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.albert.weightapp.BenchActivity">
        </activity>
        <activity android:name="com.albert.weightapp.SquatActivity">
        </activity>
        <activity android:name="com.albert.weightapp.OHPActivity">
        </activity>
        <activity android:name="com.albert.weightapp.DeadliftActivity">
        </activity>
        <activity android:name="com.albert.weightapp.BarbellRowActivity">
        </activity>
    </application>

</manifest>